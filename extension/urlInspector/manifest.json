{
  "manifest_version": 2,

  "name": "URL Inspector",
  "description": "Detect and block malicious/phishing URLs. See Readme for details.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "html/popup.html",
    "default_title": "URL Inspector"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],

  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "css": ["css/content_style.css"],
      "js": ["js/jquery2.js",                // Jquery
             "js/machine_learning.min.js",   // ML Library
             "js/URI.min.js",   // ML Library
             "js/complex_suffix_list.js",             
             "js/bloomfilter.js",
             "js/alexa_bloom_filter.js",
             "js/url_util.js",
             "js/dtmodel.js",
             "js/classifier_client.js",
             "js/content_script.js"]
    }
  ]
}